#
# nek parameter file
#
[GENERAL] 
stopAt = numSteps
numSteps = 200
dt = 1e-02
timeStepper = bdf2
extrapolation = OIFS
targetCFL = 1.9

writeControl = timeStep
writeInterval = 5

[PROBLEMTYPE]
equation = incompNS

[PRESSURE]
residualTol = 1e-05
residualProj = yes

[VELOCITY]
residualTol = 1e-06
density = 1
viscosity = -1600
